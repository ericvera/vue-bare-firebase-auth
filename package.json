    "-- PRE-COMMIT HOOKS --": "",
    "localAfterInstall": "husky || true",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "packageManager": "yarn@4.6.0",
  "peerDependencies": {
    "firebase": "^11.10.0",
    "pinia": "^2.1.0",
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@tsconfig/strictest": "^2.0.5",
    "@types/node": "^22.10.10",
    "@vitejs/plugin-vue": "^5.2.1",
    "eslint": "^9.19.0",
    "firebase": "^11.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "pinia": "^2.3.1",
    "pinst": "^3.0.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.21.0",
    "vite": "^6.0.11",
    "vite-plugin-dts": "^4.5.0",
    "vue": "^3.5.13"
  },
  "prettier": {
